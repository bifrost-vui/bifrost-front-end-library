<a
  class="btn btn-{{ buttonSize|default(
    "md"
  ) }} btn-outline-{{ buttonColor|default("dark") }}  {{ class|default("") }}"
  {{
  label is not empty
    ? "data-title='"
      ~ (label
        |render
        |t(
          [],
          {
            langcode: "EN",
            context: "data-title-attribute"
          }
        ))
      ~ "'"
  }}
  href="{{ href|default("#") }}"
  {{ extraAttribute|default(null) }}
>
  {{ label|default("Link button label") }}
</a>
