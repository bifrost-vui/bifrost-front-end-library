{% set theme = theme|json_parse|default("videotron") %}

{# Template #}
{% embed "@bf-components/navigation-header/twig/index.twig" with {
  accountData: accountData,
  langcode: langcode,
  logo    : logo,
  mainMenuItems: mainMenuItems,
  reversed: reversed,
  theme   : theme,
} only %}
{% endembed %}

