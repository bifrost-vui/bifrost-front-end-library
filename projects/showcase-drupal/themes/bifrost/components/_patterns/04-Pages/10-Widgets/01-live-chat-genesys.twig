                    buttonClass:"btn-primary btn--condensed ",
                    buttonLabel:"DEMO",
                    dataToggle:"modal",
                    dataTarget:"#modalContactUs"
                } %}
            </div>

            <div>
                <h1 class="mb-4">Genesys widget</h1>
                <hr class="border-width-4">

                <h3 class="mt-5 mb-3">Console debug</h3>
                {% include '@atoms/02-buttons/10-button-outline.twig' with {
                    buttonClass:"mr-2 w-100--mobile-only mb-2",
                    buttonColor: "",
                    buttonLabel:"Open console",
                    extraAttribute: 'onclick=javascript:CXBus.command("Console.open");'
                } %}

                <hr>

                <h3 class="mt-5 mb-3">Click on any button below to trigger the designated API event</h3>
                <div class="mb-4">
                    {#{% include '@atoms/02-buttons/10-button-outline.twig' with {#}
                        {#buttonClass:"mr-2 w-100--mobile-only mb-2",#}
                        {#buttonColor: "",#}
                        {#buttonLabel:"Toaster",#}
                        {#extraAttribute: 'onclick=javascript:CXBus.command("Toaster.open");'#}
                    {#} %}#}

                    {% include '@atoms/02-buttons/10-button-outline.twig' with {
                        buttonClass:"mr-2 w-100--mobile-only mb-2",
                        buttonColor: "",
                        buttonLabel:"Callback",
                        extraAttribute: 'onclick=javascript:CXBus.command("Callback.open");'
                    } %}

                    {% include '@atoms/02-buttons/10-button-outline.twig' with {
                        buttonClass:"mr-2 w-100--mobile-only mb-2",
                        buttonColor: "",
                        buttonLabel:"Call us",
                        extraAttribute: 'onclick=javascript:CXBus.command("CallUs.open");'
                    } %}

                    {% include '@atoms/02-buttons/10-button-outline.twig' with {
                        buttonClass:"mr-2 w-100--mobile-only mb-2",
                        buttonColor: "",
                        buttonLabel:"Channel selector",
                        extraAttribute: 'onclick=javascript:CXBus.command("ChannelSelector.open");'
                    } %}

                    {% include '@atoms/02-buttons/10-button-outline.twig' with {
                        buttonClass:"mr-2 w-100--mobile-only mb-2",
                        buttonColor: "",
                        buttonLabel:"Web chat",
                        extraAttribute: 'onclick=javascript:CXBus.command("WebChat.open");'
                    } %}

                    {% include '@atoms/02-buttons/10-button-outline.twig' with {
