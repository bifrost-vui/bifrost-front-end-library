    "chromatic": "^6.7.3",
    "css-loader": "^6.7.1",
    "cssnano": "^5.1.12",
    "ejs": "^3.1.8",
    "express": "^4.18.1",
    "extract-loader": "^5.1.0",
    "global": "^4.4.0",
    "inquirer": "^8.2.4",
    "inquirer-select-directory": "^1.2.0",
    "jasmine-core": "^4.3.0",
    "jasmine-spec-reporter": "^7.0.0",
    "jquery": "^3.6.0",
    "mini-css-extract-plugin": "^2.6.1",
