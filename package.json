{
  "name": "bifrost",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "ng build --prod && cp -R projects/front-end-library/src/lib dist/front-end-library/source",
    "e2e": "ng e2e",
    "ng": "ng",
    "start": "ng serve",
    "publish": "npm run build && npm publish dist/front-end-library",
    "storybook": "npm run twig & npm run storybook:prebuild && start-storybook -p 9008 -s public",
    "prebuild-storybook": "npm run storybook:prebuild",
    "build-storybook": "build-storybook -s public",
    "storybook:prebuild": "npm run test:generate-output && npm run docs:json",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
    "twig": "nodemon server/server.js --watch projects/ -e twig",
    "chromatic": "npx chromatic --project-token=2a5bytxelfi",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:generate-output": "jest --json --config=jest.addon-config.js --outputFile=addon-jest.testresults.json || true",
    "test:watch": "jest --watch"
  },
  "dependencies": {
