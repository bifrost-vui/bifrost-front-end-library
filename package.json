{
  "name": "bifrost-front-end-library-storybook",
  "description": "Videotron Front-End Visual Language documentation-storybook.",
  "version": "1.0.6",
  "private": true,
  "license": "proprietary",
  "homepage": "https://bifrost-front-end-library-vui.vercel.app/",
  "repository": {
    "type": "git",
    "url": "https://github.com/bifrost-vui/bifrost-front-end-library.git"
  },
  "bugs": {
    "url": "https://github.com/bifrost-vui/bifrost-front-end-library/issues"
  },
  "engines": {
    "node": "16.x",
    "npm": "8.x"
  },
  "author": "Batistemac",
  "contributors": [
    {
      "name": "JonathanLefebvre"
    }
  ],
  "scripts": {
    "start-dev": "run-p twig:server webpack:watch storybook:prebuild storybook:start",
    "twig:server": "nodemon api/server.js --watch projects/ --watch public/ -e twig",
    "build": "node scripts/generate-svg-ts --no-prompt && ng build --configuration production && npm run webpack:build && cp -R projects/front-end-library/src/lib dist/front-end-library/source && cp -R public dist/front-end-library/assets && rm -rf dist/front-end-library/assets/logo",
    "publish": "npm run webpack:clear && npm run build && npm publish ./dist/front-end-library --access public",
    "storybook:start": "node scripts/generate-svg-ts --no-prompt & start-storybook -p 9008 -s public",
    "storybook:build": "node scripts/generate-svg-ts --no-prompt && npm run webpack:build && npm run storybook:prebuild && build-storybook -s public",
    "storybook:prebuild": "npm run docs:json",
    "docs:json": "compodoc -p .storybook/tsconfig.json -e json -d .",
    "webpack:build": "webpack --mode=production --env production",
    "webpack:dev": "webpack --mode=development --env development",
    "webpack:watch": "webpack --mode=development --env development --watch",
    "webpack:clear": "rm -rf public/css && rm -rf public/js && npm run bifrost:generate-svg-ts-clear",
    "showcase-drupal:start": "cd projects/showcase-drupal && php ./core/scripts/drupal quick-start bifrost",
    "showcase-angular:start": "ng serve showcase-angular --open",
    "bifrost:generate": "node scripts/generate",
    "bifrost:generate-svg-ts": "node scripts/generate-svg-ts",
    "bifrost:generate-svg-ts-clear": "rm projects/front-end-library/src/lib/components/icon/svg/svg.ts"
  },
  "dependencies": {
    "@angular-devkit/architect": "^0.1202.10",
    "@angular-devkit/build-ng-packagr": "^0.1002.0",
    "@angular/common": "^12.2.1",
    "@angular/compiler": "^12.2.1",
    "@angular/core": "^12.2.1",
    "@angular/forms": "^12.2.1",
    "@angular/platform-browser": "^12.2.1",
    "@angular/platform-browser-dynamic": "^12.2.1",
    "@angular/router": "^12.2.1",
    "@ngrx/store": "^12.2.1",
    "@storybook/addon-docs": "^6.5.13",
    "@storybook/addon-postcss": "^2.0.0",
    "@videotron-ui/bifrost-front-end-library": "file:./dist/front-end-library",
    "core-js": "^3.0.1",
    "npx": "^10.2.2",
    "react": "^16.13.1",
    "rxjs": "^6.3.3",
    "rxjs-compat": "^6.4.0",
    "shelljs": "^0.8.5",
    "twig": "^1.15.4",
    "zone.js": "^0.11.7"
  },
