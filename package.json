  "scripts": {
    "start-dev": "npm run twig:server & npm run webpack:watch & npm run storybook:prebuild && npm run storybook:start",
    "twig:server": "nodemon api/server.js --watch projects/ --watch public/ -e twig",
    "build": "ng build --prod && npm run webpack:build && cp -R projects/front-end-library/src/lib dist/front-end-library/source && cp -R public dist/front-end-library/assets && rm -rf dist/front-end-library/assets/logo",
    "publish": "npm run build && npm publish dist/front-end-library --access public",
    "storybook:start": "start-storybook -p 9008 -s public",
    "storybook:build": "npm run webpack:build && npm run storybook:prebuild && build-storybook -s public",
    "storybook:prebuild": "npm run docs:json",
    "docs:json": "compodoc -p .storybook/tsconfig.json -e json -d .",
    "bf:generate": "node scripts/generate",
    "webpack:build": "webpack --mode=production --env.production",
    "webpack:dev": "webpack --mode=development --env.development",
    "webpack:watch": "webpack --mode=development --env.development --watch",
    "webpack:clear": "rm -rf public/css && rm -rf public/js"
  },
